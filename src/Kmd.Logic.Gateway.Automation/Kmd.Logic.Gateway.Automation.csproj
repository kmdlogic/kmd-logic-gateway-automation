<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netcoreapp3.1</TargetFramework>
	</PropertyGroup>

	<PropertyGroup>
		<Description>Library for KMD Logic Gateway Automation</Description>
		<VersionPrefix>0.1.0</VersionPrefix>
		<Authors>Logic</Authors>
		<Company>KMD</Company>
		<Product>Logic</Product>
		<PackageTags>kmd;logic;Gateway</PackageTags>
		<PackageIconUrl>https://console.kmdlogic.io/favicon-32x32.png</PackageIconUrl>
		<PackageProjectUrl>https://github.com/kmdlogic/kmd-logic-gateway-automation</PackageProjectUrl>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/kmdlogic/kmd-logic-gateway-automation</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<GenerateAssemblyVersionAttribute>false</GenerateAssemblyVersionAttribute>
	</PropertyGroup>
	
	<ItemGroup>
		<PackageReference Include="Microsoft.Rest.ClientRuntime" Version="2.3.21" />
		<PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
		<PackageReference Include="Kmd.Logic.Identity.Authorization" Version="2.0.0" />
	</ItemGroup>

</Project>
